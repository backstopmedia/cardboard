     <RmGLESShader NAME="Fragment Program" API="OpenGL ES" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
precision mediump float;
uniform sampler2D reflection_sampler;
uniform sampler2D normal;

varying vec2 vTextureCoord0;
varying vec2 vTextureCoord1;
varying float vFogAmount;
varying vec4 vClipReflection;
varying vec4 vFogColor;

void main()
{
   const float WAVE_DISTORTION = 0.04;
   const float CLAMP_MIN = 1.0 / 128.0;
   const float CLAMP_MAX = 1.0 - CLAMP_MIN;

   vec2 vDeviceReflection = vClipReflection.st / vClipReflection.q;
   vec2 vTextureReflection = vec2(0.5, 0.5) + 0.5 * vDeviceReflection;

   lowp vec3 vAccumulatedNormal = texture2D(normal, vTextureCoord0).rgb;
   vAccumulatedNormal += texture2D(normal, vTextureCoord1).rgb;
   //vAccumulatedNormal -= 1.0; // Same as * 2.0 - 2.0
   vAccumulatedNormal -= 0.5; // Same as * 2.0 - 2.0

   lowp vec2 vTmp = vAccumulatedNormal.xz;
   vTmp.x *= 0.2;
   //vec2 vTextureReflection = varTextureReflection;
   vTextureReflection.xy -= vTmp * WAVE_DISTORTION;

   vec4 reflectionTextureColor = texture2D (reflection_sampler, vTextureReflection);

   gl_FragColor = mix(reflectionTextureColor, vFogColor, vFogAmount);
   //gl_FragColor = vec4(vAccumulatedNormal, 1.0);
}        ]]></RmShaderCode></RmGLESShader>
     <RmGLESShader NAME="Vertex Program" API="OpenGL ES" PIXEL_SHADER="FALSE"><RmShaderCode TYPE="TEXT"><![CDATA[
attribute vec4 vertex;
attribute vec2 aTextureCoord;

uniform mat4 o2v_projection;
uniform float time;
uniform mat4 o2v_projection_reflection;
uniform mat4 view_matrix;

varying vec2 vTextureCoord0;
varying vec2 vTextureCoord1;
varying float vFogAmount;
varying vec4 vClipReflection;
varying vec4 vFogColor;

void main()
{
const vec4 lightDir = vec4(-0.359010, -0.311410, 0.879850, 0.0);
const vec4 fogColor_1 = vec4(0.5216, 0.7068, 0.8632, 1.0);
const vec4 fogColor_2 = vec4(0.1059, 0.1647, 0.2549, 1.0);
const float fogDistance = 800.0;
const float fogStartDistance = 100.0;
const float ZFOG_OFFSET = -800.0;
const float ZFOG_COEFFICIENT = -0.003;
const float n_specular = 30.8;
const float Ks = 5.0;

   const lowp vec2 BUMP_SCALE0 = vec2(1.2, 1.2) * 1.0;
   const lowp vec2 BUMP_SCALE1 = vec2(0.5, 0.5) * 1.0;
   const lowp vec2 BUMP_VELOCITY0 = vec2(0.16, 0.14) * 0.15;
   const lowp vec2 BUMP_VELOCITY1 = vec2(0.25, 0.3) * 0.2;

   gl_Position = o2v_projection * vec4(vertex.xy, 0.0, 1.0);

   vTextureCoord0 = aTextureCoord * BUMP_SCALE0;
   vTextureCoord0 += time * BUMP_VELOCITY0;
   vTextureCoord1 = aTextureCoord * BUMP_SCALE1;
   vTextureCoord1 += time * BUMP_VELOCITY1;

   vClipReflection = o2v_projection_reflection * vec4(vertex.xy, 0.0 , 1.0);

   float height_term = clamp((vertex.z + ZFOG_OFFSET) * ZFOG_COEFFICIENT, 0.0, 1.0);
   float depth_term = clamp((length(gl_Position) - fogStartDistance) / fogDistance, 0.0, 1.0);
   vFogAmount = depth_term * height_term; 

   vec3 vLightVec   = -lightDir.xyz;
   vLightVec.z = -vLightVec.z;    // notice that we need to flip the z here to let OGL match D3D
   
   // Transform vertex position into view space:
   vec3 Pview =  vec3(view_matrix * vertex);

   // Compute view vector (view space):
   vec3 vViewVec = -normalize(Pview);

   float sunAmount = clamp(Ks * pow( max( 0.0, dot(vLightVec, vViewVec)), n_specular ), 0.0, 1.0);
   vFogColor  = mix(fogColor_2, fogColor_1, sunAmount);
}]]></RmShaderCode></RmGLESShader>
