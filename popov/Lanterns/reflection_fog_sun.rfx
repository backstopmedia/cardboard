   vec2 scaledDistFromZAxis = vec2(sqrt(dot(noisyWobblyTCoord.xy, noisyWobblyTCoord.xy)) * ring_freq, 0.5);

   vec4 blendFactor = texture2D(pulse_train, scaledDistFromZAxis);
 
    // Blend wood colors together
   vec4 albedo = mix(dark_wood_color, light_wood_color, blendFactor.x);
   
   // Compute normalized vector from vertex to light in eye space  (Leye)
   vec3 Leye = normalize(light_pos.xyz - vPositionES);

   // Normalize interpolated normal
   vec3 Neye = normalize(vNormalES);

    // Compute Veye
   vec3 Veye = -normalize(vPositionES);

    // Compute half-angle
   vec3 Heye = normalize(Leye + Veye);

    // Compute N.H
   float NdotH = clamp(dot(Neye, Heye), 0.0, 1.0);

    // Scale and bias specular exponent from pulse train into decent range
   float k = blendFactor.z;

   // Evaluate (N.H)^k via dependent read
   float specular = texture2D (variable_specular, vec2(NdotH, k)).x;

   // N.L
   float NdotL = dot(Neye, Leye);

   // "Half-Lambert" technique for more pleasing diffuse term
   float diffuse = NdotL * 0.5 + 0.5;

   // gloss the specular term
   float gloss = blendFactor.y;

   gl_FragColor = diffuse * albedo + specular * gloss;
   
}]]></RmShaderCode></RmGLESShader>
     <RmGLESShader NAME="Vertex Program" API="OpenGL ES" PIXEL_SHADER="FALSE"><RmShaderCode TYPE="TEXT"><![CDATA[
uniform float trunk_wobble_frequency;
uniform mat4 texture_matrix0;
uniform mat4 texture_matrix1;
uniform mat4 texture_matrix2;

uniform mat4 matViewInverseTranspose;
uniform mat4 view_matrix;
uniform mat4 view_proj_matrix;

attribute vec4 rm_Vertex;
attribute vec3 rm_Normal;

varying vec3 vTCoord0;
varying vec3 vTCoord1;
varying vec3 vTCoord2;
varying vec3 vZWobble0;
varying vec3 vZWobble1;
varying vec3 vPositionES;
varying vec3 vNormalES;

void main(void)
{
   gl_Position = view_proj_matrix * rm_Vertex;
   
   // Transform Pshade (using texture matrices)
   // Note: GLSL expects matrixes to be column major, RM stores matrices row major.
   vTCoord0 = vec3( texture_matrix0 * rm_Vertex);
   vTCoord1 = vec3( texture_matrix1 * rm_Vertex);
   vTCoord2 = vec3( texture_matrix2 * rm_Vertex);

   // Create two coordinates for sampling noise volume to get wobble
   vZWobble0 = vec3(trunk_wobble_frequency * vTCoord0.z, 0.0, 0.0);
   vZWobble1 = vec3(trunk_wobble_frequency * vTCoord0.z + 0.5, 0.0, 0.0);

   // Transform position and normal to eye space
   vPositionES  = vec3(view_matrix * rm_Vertex);
   vNormalES    = vec3(matViewInverseTranspose * vec4(rm_Normal, 0.0));

}]]></RmShaderCode></RmGLESShader>
