     <RmGLESShader NAME="Vertex Program" API="OpenGL ES" PIXEL_SHADER="FALSE"><RmShaderCode TYPE="TEXT"><![CDATA[
precision mediump float;

uniform mat4 uMVPMatrix;
uniform mat4 view_matrix;
/*uniform vec4 lightDir;
uniform vec4 fogColor_1;
uniform vec4 fogColor_2;
uniform float fogDistance;// = 800.0;
uniform float fogStartDistance;// = 100.0;
uniform float ZFOG_OFFSET;// = -800.0;
uniform float ZFOG_COEFFICIENT;// = -0.003;
uniform float n_specular;// = 30.8;
uniform float Ks;// = 5.0;*/

attribute vec4 aPosition;
attribute vec2 aTextureCoord;

varying vec2 vTextureCoord;
varying float vFogAmount;
varying vec4 vFogColor;

void main(){
const vec4 lightDir = vec4(-0.359010, -0.311410, 0.879850, 0.0);
const vec4 fogColor_1 = vec4(0.5216, 0.7068, 0.8632, 1.0);
const vec4 fogColor_2 = vec4(0.1059, 0.1647, 0.2549, 1.0);
const float fogDistance = 800.0;
const float fogStartDistance = 100.0;
const float ZFOG_OFFSET = -800.0;
const float ZFOG_COEFFICIENT = -0.003;
const float n_specular = 30.8;
const float Ks = 5.0;

   gl_Position = uMVPMatrix * aPosition;
   vTextureCoord = aTextureCoord;

   float height_term = clamp((aPosition.z + ZFOG_OFFSET) * ZFOG_COEFFICIENT, 0.0, 1.0);
   float depth_term = clamp((length(gl_Position) - fogStartDistance) / fogDistance, 0.0, 1.0);
   vFogAmount = depth_term * height_term; 
   
   vec3 vLightVec   = -lightDir.xyz;
   vLightVec.z = -vLightVec.z;    // notice that we need to flip the z here to let OGL match D3D
   
   // Transform vertex position into view space:
   vec3 Pview =  vec3(view_matrix * aPosition);

   // Compute view vector (view space):
   vec3 vViewVec = -normalize(Pview);

   float sunAmount = clamp(Ks * pow( max( 0.0, dot(vLightVec, vViewVec)), n_specular ), 0.0, 1.0);
   vFogColor  = mix(fogColor_2, fogColor_1, sunAmount);
}
]]></RmShaderCode></RmGLESShader>
     <RmGLESShader NAME="Fragment Program" API="OpenGL ES" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
precision mediump float;

uniform sampler2D sTexture;
uniform float textureLod;

varying vec4 vFogColor;
varying vec2 vTextureCoord;
varying float vFogAmount;

void main() {
  vec4 base = texture2DLod(sTexture, vTextureCoord, textureLod);

  gl_FragColor = mix(base, vFogColor, vFogAmount);
}
]]></RmShaderCode></RmGLESShader>
